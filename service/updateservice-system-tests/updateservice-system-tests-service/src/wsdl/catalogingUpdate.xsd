<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://oss.dbc.dk/ns/catalogingUpdate" elementFormDefault="qualified" targetNamespace="http://oss.dbc.dk/ns/catalogingUpdate">
  <!-- updateRecord request / response -->
  <xs:element name="updateRecord">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="updateRecordRequest" type="tns:UpdateRecordRequest"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="UpdateRecordRequest" type="tns:UpdateRecordRequest"/>
  <xs:complexType name="UpdateRecordRequest">
    <xs:sequence>
      <xs:element ref="tns:agencyId"/>
      <xs:element ref="tns:validateSchema"/>
      <xs:element name="bibliographicRecord" type="tns:BibliographicRecord"/>
      <xs:element name="options" type="tns:Options" minOccurs="0"/>
      <xs:element ref="tns:trackingId" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="updateRecordResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="updateRecordResult" type="tns:UpdateRecordResult"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="UpdateRecordResult" type="tns:UpdateRecordResult"/>
  <xs:complexType name="UpdateRecordResult">
    <xs:sequence>
      <xs:element ref="tns:updateStatus"/>
      <xs:element name="validateInstance" type="tns:ValidateInstance" minOccurs="0"/>
      <xs:element name="bibliographicRecord" type="tns:BibliographicRecord" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">If the update/validation has altered the record, the corrected records is returned here. Currently empty</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!-- getValidateSchemas request / response -->
  <xs:element name="getValidateSchemas">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="getValidateSchemasRequest" type="tns:GetValidateSchemasRequest"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GetValidateSchemasRequest" type="tns:GetValidateSchemasRequest"/>
  <xs:complexType name="GetValidateSchemasRequest">
    <xs:sequence>
      <xs:element ref="tns:agencyId"/>
      <xs:element ref="tns:trackingId" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="getValidateSchemasResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="getValidateSchemasResult" type="tns:GetValidateSchemasResult"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GetValidateSchemasResult" type="tns:GetValidateSchemasResult"/>
  <xs:complexType name="GetValidateSchemasResult">
    <xs:sequence>
      <xs:element name="schema" type="tns:Schema" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="tns:validateSchemasStatus"/>
    </xs:sequence>
  </xs:complexType>

  <!-- structs -->
  <xs:element name="BibliographicRecord" type="tns:BibliographicRecord"/>
  <xs:complexType name="BibliographicRecord">
    <xs:sequence>
      <xs:element ref="tns:recordSchema"/>
      <xs:element ref="tns:recordPacking"/>
      <xs:element name="recordData" type="tns:RecordData"/>
      <xs:element name="extraRecordData" type="tns:ExtraRecordData" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ExtraRecordData" type="tns:ExtraRecordData"/>
  <xs:complexType name="ExtraRecordData" mixed="true">
    <xs:annotation>
      <xs:documentation xml:lang="en">Extra data for the update - normally not used.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Options" type="tns:Options"/>
  <xs:complexType name="Options">
    <xs:sequence>
      <xs:element ref="tns:option" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Schema" type="tns:Schema"/>
  <xs:complexType name="Schema">
    <xs:sequence>
      <xs:element ref="tns:validateSchemaName"/>
      <xs:element ref="tns:validateSchemaInfo" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="RecordData" type="tns:RecordData"/>
  <xs:complexType name="RecordData" mixed="true">
    <xs:annotation>
      <xs:documentation xml:lang="en">The record to update</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ValidateEntry" type="tns:ValidateEntry"/>
  <xs:complexType name="ValidateEntry">
    <xs:sequence>
      <xs:element ref="tns:warningOrError"/>
      <xs:element ref="tns:urlForDocumentation" minOccurs="0"/>
      <xs:element ref="tns:ordinalPositionOfField" minOccurs="0"/>
      <xs:element ref="tns:ordinalPositionOfSubField" minOccurs="0"/>
      <xs:element ref="tns:message" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ValidateInstance" type="tns:ValidateInstance"/>
  <xs:complexType name="ValidateInstance">
    <xs:sequence>
      <xs:element name="validateEntry" type="tns:ValidateEntry" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <!-- types -->
  <xs:element name="agencyId" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="en">Library code.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="message" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="en">Explanation of the warning or error</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="option" type="tns:UpdateOptionEnum">
    <xs:annotation>
      <xs:documentation xml:lang="en">Option to alter the update.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ordinalPositionOfField" type="xs:integer">
    <xs:annotation>
      <xs:documentation xml:lang="en">Ordinal position of field in record. -1 if not apllicable</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ordinalPositionOfSubField" type="xs:integer">
    <xs:annotation>
      <xs:documentation xml:lang="en">Ordinal position of subfield in field. -1 if not apllicable</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="recordPacking" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="en">Defaults to xml.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="recordSchema" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="en">Defaults to: info:lc/xmlns/marcxchange-v1</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="trackingId" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="en">Unique id to track this request</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="urlForDocumentation" type="xs:anyURI">
    <xs:annotation>
      <xs:documentation xml:lang="en">url to documentation (danMarc2)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="updateStatus" type="tns:UpdateStatusEnum">
    <xs:annotation>
      <xs:documentation xml:lang="en">Status for the update.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="validateSchema" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="en">Schema for validating the bibliographic record.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="validateSchemaInfo" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="en">Information about validate schema.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="validateSchemaName" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="en">Name of validate schema.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="validateSchemasStatus" type="tns:ValidateSchemasStatusEnum">
    <xs:annotation>
      <xs:documentation xml:lang="en">Status for the get validate schemas.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="warningOrError" type="tns:ValidateWarningOrErrorEnum">
    <xs:annotation>
      <xs:documentation xml:lang="en">Error type or warning</xs:documentation>
    </xs:annotation>
  </xs:element>

  <!-- enums -->
  <xs:simpleType name="ValidateWarningOrErrorEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="error"/>
      <xs:enumeration value="warning"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="UpdateOptionEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="validate_only"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="UpdateStatusEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ok"/>
      <xs:enumeration value="validate_only"/>
      <xs:enumeration value="validation_error"/>
      <xs:enumeration value="failed_invalid_agency"/>
      <xs:enumeration value="failed_invalid_schema"/>
      <xs:enumeration value="failed_invalid_option"/>
      <xs:enumeration value="failed_validation_internal_error"/>
      <xs:enumeration value="failed_update_internal_error"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ValidateSchemasStatusEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ok"/>
      <xs:enumeration value="failed_internal_error"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
